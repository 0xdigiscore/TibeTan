<?xml version="1.0" encoding="utf-8"?>
<keyboard
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="67dp"
     >
    <row>
        <key keyIcon="@drawable/hw_backspace"  codes="67" repeat="true" />
    </row>
    <row>
        <key keyIcon="@drawable/hw_space" code="62" repeat="true" key_type="13">
            <toggle_state keyIcon="@drawable/hw_space"  />
            <toggle_state keyIcon="@drawable/hw_other"  />
        </key>
    </row>
    <row>
        <key keyIcon="@drawable/hw_enter"  code="66" repeat="true" />
    </row>
    <row>
        <key icon="@drawable/hw_123" icon_popup="@drawable/number123" key_type="22" code="-8" width="11.875%p" height="25%p" />
        <key icon="@drawable/hw_swtich" icon_popup="@drawable/hw_switch_hl" code="-2" width="9.375%p" height="25%p" key_type="12" />
    </row>
</keyboard>